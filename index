        <button x-show="$store.modal.transcript" class="text-btn text-white uppercase tracking-widest px-em py-xs" @click="showTranscript($event)" x-text="$store.modal.showTranscript ? 'Hide Transcript' : 'Show Transcript'">Show Transcript</button>
      </div>
      <div class="relative -top-ms bg-white text-segment w-full overflow-auto px-sm" :class="modalClass()"  x-ref="transcript">
        <div x-show="$store.modal.showTranscript" x-collapse.duration.700ms>
          <div class="py-md md:px-md" id="modal-transcript" tabindex="0">
          Transcipt text goes here <a href="#">Test link</a>;
          </div>
        </div>
      </div>
    </div>

